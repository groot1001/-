/**app.wxss**/

page {
  /* color: #333 */
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.contact {
  font-size: 34rpx;
  color: #fff;
  background-color: #3c87ca;
  line-height: 100rpx;
  height: 100rpx;
  justify-content: center;
  display: flex;
  width: 100%;
  position: fixed;
  bottom: 0;
  align-items: center;
  border-radius: 0;
}

.nocourse {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ccc;
  font-size: 36rpx;
  margin-top: 20%;
}

.contact::after {
  border: 0;
}
.page_bottom{
  height: 80rpx;
  display: flex;
  align-items: center;
  width: 100%;
  background-color: #f2f6f9;
  color: #ccc;
  font-size: 24rpx;
  justify-content: center;
}
.bottom_line{
  height: 2rpx;
  background-color: #ccc;
  width: 100rpx;
}
.bottom_tips{
  padding: 0 12rpx;
}


/* 弹框 */
.modal_box{
  width: 100%;
  position: absolute;top: 0;
  left: 0;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.4);
  display: flex;
  align-items: center;
  justify-content: center;
  
}
.modal_item{
  width: 600rpx;
  border-radius: 10rpx;
  background-color: #Fff;
  
}
.modal_item_content{
  /* height: 100rpx; */
  width: 100%;
  text-align: center;
}
.modal_tit{
  font-size: 36rpx;
  color: #333;
  
  padding: 22rpx 0;
  display: block;
}
.modal_txt{
  font-size: 28rpx;
  color: #999;
}
.modal_item_bottom{
  background:linear-gradient(to right, #3396ef , #0095f6);
  color: #fff;
  font-size: 28rpx;
  border-radius: 30rpx;
  height: 60rpx;
  width:80%;
  margin: 30rpx auto; 
  line-height: 60rpx;
  text-align: center;
}
.gb{
  position: absolute;
  color: #fff;
  bottom:30%;
  left: 48.5%；
}

