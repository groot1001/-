  height: 80rpx;
  line-height: 80rpx;
  justify-content: right;
}

.scroll_item {
  display: inline-block;
  padding: 0rpx 22rpx;
  /* line-height: 50rpx; */
  font-size: 26rpx;
  position: relative;
}

.scroll_item2 {
  width: 148rpx;
  display: inline-block;
  /* height: 80rpx; */
}

.hot_class {
text-align: center;
  position: absolute;
  top: -28rpx;
  left: 24px;
  color: #f00 !important;
  font-size: 46rpx !important;
  z-index: 99;
  animation: myfirst 1s infinite 0.6s;
  -moz-animation: myfirst 1s infinite 0.6s;
  /* Firefox */
  -webkit-animation: myfirst 1s infinite 0.6s;
  /* Safari and Chrome */
  -o-animation: myfirst 1s infinite 0.6s;
}

    @keyframes myfirst {
        0% {
           font-size: 42rpx;
        }

        50% {
            font-size: 46rpx;
        }
        100% {
            font-size: 42rpx;
        }
    }

