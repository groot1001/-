/* banner */
@import "../answertemplate/answertemplate.wxss";

.gz-ul {
  margin-top: 0;
}

.swiper {
  width: 100%;
  height: 376rpx;
  background-color: #f0f0f0;
}

.swiper .wx-swiper-dot {
  /* 修改原生样式 */
  border: 2rpx solid #fff !important;
}

.swiper image {
  width: 100%;
  height: 100%;
  background-color: #f0f0f0;
}

.ywfw-view {
  text-align: center;
  padding-top: 30rpx;
}

.yewf-title {
  font-size: 46rpx;
  color: #000;
  font-weight: 900;
  font-family: "微软雅黑";
}

.ad_contact {
  padding: 0;
  border-radius: 0;
  line-height: 0;
  margin: 0;
}

.ywfw-eight {
  padding: 0 10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.ywfw-first {
  width: 24%;
  margin: 0 1% 20rpx 0;
  box-sizing: border-box;
  text-align: center;
  padding-top: 20rpx;
}

.ywfw-first image {
  width: 80%;
  height: 145rpx;
  margin: 0 10%;
}

.ywfw-first text {
  font-size: 26rpx;
  color: #000;
  font-weight: 0;
}

/* 三大块 */

.sandakuai {
  width: 100%;
  padding: 30rpx 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.sandakuai .sdk-view {
  width: 25%;
  text-align: center;
  display: flex;
  flex-direction: column;
}

.sandakuai .sdk-view image {
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}

.sandakuai .sdk-view text {
  font-size: 26rpx;
  margin-top: 15rpx;
}

/* 横线 */

.hx-first {
  width: 100%;
  height: 12rpx;
  background: #f4f5f9;
}

/* 关于我们 */

.index-about {
  padding: 40rpx 30rpx 30rpx 30rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  text-align: center;
}

.index-about .text1 {
  font-size: 34rpx;
  color: #3c87ca;
  font-weight: bold;
}

.index-about .text2 {
  font-size: 24rpx;
  color: #999;
  margin-top: 12rpx;
}

/* 关于我们-分类 */

.about-ul {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 30rpx;
}

.about-ul .about-li {
  display: flex;
  flex-direction: column;
  text-align: center;
  width: 25%;
  padding-top: 24rpx;
}

.about-ul .shux {
  border-right: 1px solid #f0f0f0;
}

.about-ul .about-li .img1 {
  width: 47rpx;
  height: 53rpx;
  margin: 0 auto;
}

.about-ul .about-li .img2 {
  width: 49rpx;
  height: 49rpx;
  margin: 0 auto;
}

.about-ul .about-li .img3 {
  width: 52rpx;
  height: 45rpx;
  margin: 0 auto;
}

.about-ul .about-li .img4 {
  width: 46rpx;
  height: 49rpx;
  margin: 0 auto;
}

.about-ul .about-li text {
  font-size: 25rpx;
  color: #000;
  margin-top: 16rpx;
}

/* 文字滚动 */

.swiper_container {
  height: 100rpx;
}

.notice {
  width: 100%;
  padding: 18rpx 30rpx 18rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  position: relative;
}

.notice .img1 {
  width: 70rpx;
  height: 67rpx;
  margin: 30rpx 35rpx 0 8rpx;
}

.notice .notice-ul {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.notice .notice-ul text {
  font-size: 24rpx;
  color: #333;
  margin: 4rpx 0;
}

.notice .notice-ul text .time-font {
  color: #a2a2a2;
  margin-left: 16rpx;
}

.notice .right-jt {
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 30rpx;
  margin: auto;
}

/* 推荐课程 */

.tj-class {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.tj-class .class-tit {
  width: 100%;
  text-align: center;
  padding: 30rpx 0;
  position: relative;
}

.tj-class .class-tit .text1 {
  font-size: 34rpx;
  font-weight: 600;
  font-family: "微软雅黑";
}

.tj-class .class-gd {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  line-height: 110rpx;
}

.tj-class .class-gd text {
  font-size: 24rpx;
  color: #a0a0a0;
}

.tj-class .class-gd image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 16rpx;
}

/* 课程分类 */

.tj-class .class-kec {
  display: flex;
  flex-direction: column;
  margin-bottom: 40rpx;
}

.tj-class .class-kec .class-1 {
  width: 100%;
  height: 350rpx;
}

.tj-class .class-kec .text1 {
  font-size: 30rpx;
  color: #333;
  margin-top: 20rpx;
}

.tj-class .class-kec .text2 {
  font-size: 24rpx;
