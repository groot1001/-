  margin-top: 20rpx;
}

.tj-class .class-kec .numder-peo image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.tj-class .class-kec .numder-peo .num-text {
  font-size: 24rpx;
  color: #9f9f9f;
}

